{
  "expo": {
    "name": "t-stop-pro",
    "slug": "t-stop-pro",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/car-splash.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "resizeMode": "contain",
      "backgroundColor": "#fff"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "backgroundColor": "#ffffff"
      }
    },
    "plugins": [[
      "expo-build-properties",
      {
        "ios": {
          "useFrameworks": "static"
        }
      }
    ]]
  }
}
